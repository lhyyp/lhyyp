<!--components/swiper/seiper.wxml-->
<swiper indicator-dots  indicator-color="#fff"   indicator-active-color="#f4ea2a"
   autoplay interval="2000" duration="500" circular>
  <block wx:for="{{BannerList}}">
    <swiper-item>
      <image src="{{item.src}}" class="slide-image" data-url="{{item.url}}"  bindtap='runDetail'/>
    </swiper-item>
  </block>

</swiper>
